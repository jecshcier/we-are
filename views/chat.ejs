        <div class="setWinClose"><i class="fa fa-times-circle-o" title="关闭"></i>
        </div>
        <ul>
            <li rel="userTx" class="setMenuActive">
                <i class="fa fa-twitter"></i> 头像
            </li>
            <% if(role_type === 3) { %>
            <li rel="project"><i class="fa fa-group"></i> 项目组
            </li>

