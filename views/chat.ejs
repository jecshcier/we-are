                        <button id="saveTx" style="">保存</button>
                    </div>
                </div>
                <div>
                    <ul>
                        <% for(var i = 23; i < 47; i++) { %>
                        <li>
                            <img class="defaultTx" src="<%= staticUrl %>/images/tx/<%= i %>.jpg" width="100%"
                                 height="100%"></li>
                        <% } %>
                    </ul>
                </div>
            </div>

        </div>
        <% if(role_type === 3) { %>

        <!--项目组-->
        <div style="display: none;">
            <div class="proMenu">
                <div class="addStyle addPro">
                    <i class="fa fa-plus" title="添加项目组"></i> 添加项目组
                </div>
                <div class="addStyle proReflash">
                    <i class="fa fa-refresh" title="刷新"></i> 刷新项目组
                </div>
            </div>
            <div class="programmeUl">
                <ul></ul>
            </div>
        </div>


        <div style="display: none;">
            <div class="editUser">
